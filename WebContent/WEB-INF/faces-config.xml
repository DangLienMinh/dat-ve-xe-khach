 </managed-bean>
  <managed-bean>
  <managed-bean-name>phanQuyenMBean</managed-bean-name>
  <managed-bean-class>controller.PhanQuyenManagedBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
  <managed-bean>
  <managed-bean-name>nhanVienMBean</managed-bean-name>
  <managed-bean-class>controller.NhanVienManagedBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <render-kit>
    <renderer>
        <component-family>org.primefaces.component</component-family>
        <renderer-type>org.primefaces.component.FileUploadRenderer</renderer-type>
        <renderer-class>filters.MyFileUploadRenderer</renderer-class>
    </renderer>
