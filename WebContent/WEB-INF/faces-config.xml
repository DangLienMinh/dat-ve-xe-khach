  <managed-bean-class>controller.TaiXeManagedBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
  <managed-bean>
  <managed-bean-name>taiKhoanMBean</managed-bean-name>
  <managed-bean-class>controller.TaiKhoanManagedBean</managed-bean-class>
